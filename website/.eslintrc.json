{
  "extends": [
    "@trevorblades/eslint-config/react",
    "plugin:jsx-a11y/recommended"
  ],
  "rules": {
    "react/react-in-jsx-scope": "off",
    "no-multi-spaces": ["error"]
  },
  "overrides": [
    {
      "files": ["**/__tests__/*.js", "**/__tests__/*.jsx", "**.test.js"],
      "env": {
        "jest": true
      }
    }
  ]
}
